        "framer-motion": "^12.23.6",
        "jsonwebtoken": "^9.0.2",
        "marked": "^16.1.0",
        "next": "^15.5.2",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-icons": "^5.5.0",
        "react-markdown": "^10.1.0",
