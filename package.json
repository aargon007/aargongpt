{
  "name": "aargongpt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "ts:check": "tsc --noEmit",
    "format": "prettier --write --ignore-unknown .",
    "postinstall": "prisma generate",
    "test": "jest --passWithNoTests --watchAll=false",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --passWithNoTests --watchAll=false",
    "test:unit": "jest src/__tests__ --passWithNoTests --watchAll=false",
    "test:simple": "jest src/__tests__/simple.test.js --watchAll=false",
    "test:performance": "node scripts/performance-test.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "node scripts/test-runner.js",
    "test:quick": "yarn test:simple && yarn test:performance"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.23",
    "@ai-sdk/openai": "^2.0.53",
    "@ai-sdk/react": "^2.0.78",
    "@prisma/client": "^6.18.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "ai": "^5.0.78",
    "bcrypt": "^6.0.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "jsonwebtoken": "^9.0.2",
    "marked": "^16.4.1",
    "next": "^16.0.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.0.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9",
